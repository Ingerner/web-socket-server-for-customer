<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">


    <changeSet id="2023-03-28-create-device-turning-table" author="okorotkiy">
        <createTable tableName="device_turning">
            <column name="id" type="int" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_device_turning"/>
            </column>
            <column name="mac" type="varchar(64)">
                <constraints nullable="false"/>
            </column>
            <column name="date" type="datetime"/>
        </createTable>
    </changeSet>

</databaseChangeLog>